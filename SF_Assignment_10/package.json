{
  "name": "typescript-express-postgres",
  "version": "1.0.0",
  "description": "A TypeScript project using Express and PostgreSQL",
  "scripts": {
    "dev": "concurrently 'npm run dev:server' 'npm run dev:client'",
    "dev:server": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "dev:client": "webpack serve --config webpack.config.js --mode development",
    "build": "tsc",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pg-monitor": "^2.0.0",
    "pg-promise": "^11.4.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.13",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}
